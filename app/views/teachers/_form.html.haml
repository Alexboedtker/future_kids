= render :partial => '/layouts/resource_actions'

= semantic_form_for resource, :html => { :id => 'content_form' } do |form|
  = form.semantic_errors
  = form.inputs do
    = form.input :name
    = form.input :prename
    = form.input :email
    = form.input :password
    = form.input :password_confirmation
    = form.input :school
    = form.input :address
    = form.input :phone
  = form.buttons 
